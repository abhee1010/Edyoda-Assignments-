     "text": [
      "[(2, 1), (1, 2), (2, 3), (4, 4), (2, 5)]\n"
     ]
    }
   ],
   "source": [
    "def last_digit(n):\n",
    "    return n[-1]  \n",
    " \n",
    "def sort(tuples):\n",
    "    return sorted(tuples, key=last_digit)\n",
    " \n",
    "print (sort( [(2, 5), (1, 2), (4, 4), (2, 3), (2, 1)]))"
   ]
